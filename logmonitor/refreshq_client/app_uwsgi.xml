<uwsgi>
    <socket>127.0.0.1:9090</socket>
    <pythonpath>/Application</pythonpath>
    <socket-timeout>180</socket-timeout>
    <master/>
    <listen>10240</listen>
    <processes>4</processes>
    <harakiri>60</harakiri>
    <enable-threads/>
    <post-buffering>131072</post-buffering>
    <post-buffering-bufsize>131072</post-buffering-bufsize>
    <max-requests>10240</max-requests>
    <buffer-size>65536</buffer-size>
    <module>app_uwsgi</module>
    <threads>16</threads>
    <limit-as>2048</limit-as>
    <limit-post>1048576</limit-post>
    <daemonize>/Application/refreshq_client/logs/uwsgi.log</daemonize>
    <pidfile>/tmp/refreshq_client.pid</pidfile>
</uwsgi>